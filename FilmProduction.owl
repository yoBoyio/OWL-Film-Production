#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/FilmProduction#ArtDirector
:ArtDirector rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Employee
                                                        [ rdf:type owl:Class ;
                                                          owl:complementOf :ProductionDesigner
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :worksAt ;
                                                          owl:someValuesFrom :Production
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf :Employee .


###  http://www.semanticweb.org/FilmProduction#Departments
:Departments rdf:type owl:Class ;
             rdfs:subClassOf :DomainEntity .


###  http://www.semanticweb.org/FilmProduction#Director
:Director rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Employee
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :isDirecting ;
                                                       owl:someValuesFrom :Movie
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :worksAt ;
                                                       owl:someValuesFrom :Management
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :Employee .


###  http://www.semanticweb.org/FilmProduction#DirectorOfPhotography
:DirectorOfPhotography rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :Employee
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :isManagerOf ;
                                                                    owl:someValuesFrom :Photography
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf :Employee .


