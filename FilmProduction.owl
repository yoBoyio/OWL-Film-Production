#################################################################

###  http://www.semanticweb.org/FilmProduction#hasManager
:hasManager rdf:type owl:ObjectProperty ;
            owl:inverseOf :isManagerOf ;
            rdfs:domain :Departments ;
            rdfs:range :Person .


###  http://www.semanticweb.org/FilmProduction#hasSex
:hasSex rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdf:type owl:FunctionalProperty ;
        rdfs:domain :Person ;
        rdfs:range :Sex .


###  http://www.semanticweb.org/FilmProduction#isDirectedBy
:isDirectedBy rdf:type owl:ObjectProperty ;
              owl:inverseOf :isDirecting ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :Movie ;
              rdfs:range :Director .


###  http://www.semanticweb.org/FilmProduction#isDirecting
:isDirecting rdf:type owl:ObjectProperty ;
             rdfs:domain :Director ;
             rdfs:range :Movie .


###  http://www.semanticweb.org/FilmProduction#isManagerOf
:isManagerOf rdf:type owl:ObjectProperty ;
             rdfs:domain :Employee ;
             rdfs:range :Departments .


###  http://www.semanticweb.org/FilmProduction#worksAt
:worksAt rdf:type owl:ObjectProperty ;
         rdfs:domain :Employee ;
         rdfs:range :Departments .


###  http://www.semanticweb.org/FilmProduction#worksOnMovie
:worksOnMovie rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Person ;
              rdfs:range :Movie .


